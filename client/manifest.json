{
  "manifest_version": 3,
  "name": "AI AdBuster",
  "version": "1.0",
  "description": "Turn ads hallowweennyy!!",
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "src/content.ts"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
